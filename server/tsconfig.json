{   
    "compilerOptions": {
        "baseUrl": "server/",
        "paths": {
            "@/utils/*": [
                "utils/*"
            ],
            "@/types": [
                "types/index"
            ],
        },
        "target": "es6",
        "module": "commonjs",
        "rootDir": "./",
        "outDir": "./build",
        "allowJs": true,
        "esModuleInterop": true,
        "strict": true
    }
}

